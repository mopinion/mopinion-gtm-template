___SANDBOXED_JS_FOR_WEB_TEMPLATE___

// Enter your template code here.
const log = require('logToConsole');
const query = require('queryPermission');
const callInWindow = require('callInWindow');
const injectScript = require('injectScript');
log('data =', data);

const deploymentID = data.mopinionDeploymentID;

const onSuccess = () => {
  if (query('access_globals', 'execute', 'Pastease')) {
 callInWindow('Pastease.load', deploymentID);}
 data.gtmOnSuccess();
};

const onFail = () => {
  data.gtmOnFailure();
};

const scriptURL = 'https://deploy.mopinion.com/js/pastease.js';
if (query('inject_script', scriptURL)){
  injectScript(scriptURL, onSuccess, onFail, scriptURL);
}


